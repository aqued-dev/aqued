{
	"name": "@aqued/bot",
	"version": "4.0.0-beta.0",
	"description": "Multifunctional Discord Bot.",
	"main": "dist/index.js",
	"author": "aqued-dev",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "rimraf dist && swc src -d dist && prisma migrate dev --name init generate",
		"start": "prisma migrate deploy --name init generate && node dist/src/index.js | pino-pretty -tc",
		"dev": "pnpm build && node dist/src/index.js | pino-pretty -tc",
		"lint": "eslint src --ext .ts && prettier --check .",
		"lint:fix": "eslint src --ext .ts --fix && prettier --write ."
	},
	"dependencies": {
		"@google/generative-ai": "^0.13.0",
		"@prisma/client": "^5.15.1",
		"@types/ws": "^8.5.10",
		"@xmldom/xmldom": "^0.8.10",
		"discord.js": "^14.15.3",
		"dotenv": "^16.4.5",
		"fastify": "^4.28.0",
		"pino": "^9.2.0",
		"pino-pretty": "^11.2.1",
		"prisma": "^5.15.1",
		"ws": "^8.17.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.3.12",
		"@swc/core": "^1.6.3",
		"@types/node": "^20.14.7",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-unicorn": "^54.0.0",
		"globals": "^15.6.0",
		"husky": "^9.0.11",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.7",
		"typescript-eslint": "8.0.0-alpha.30"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "pnpm@9.4.0",
	"volta": {
		"node": "22.3.0",
		"pnpm": "9.4.0"
	}
}
